{
  "name": "resume-builder",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm:start --workspace client\" \"npm:start --workspace server\"",
    "start": "concurrently \"npm:start --workspace client\" \"npm:start --workspace server\"",
    "install-all": "npm install --workspaces"
  },
  "devDependencies": {
    "concurrently": "^8.0.0"
  }
}
